{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/SydneyEvents/backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/SydneyEvents/frontend/$1"
    }
  ],
   "functions": {
    "SydneyEvents/backend/server.js": {
      "maxDuration": 10
    }
  }
}
